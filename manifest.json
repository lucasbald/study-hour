{
  "manifest_version": 2,
  "version": "1.0.0",
  "name": "Google SignIn",
  "description": "Extens√£o Google SignIn",
  "author": "Lucas Baldin",
  "key": "",
  "content_security_policy": "script-src 'self' https://apis.google.com https://www.gstatic.com https://www.googleapis.com https://securetoken.googleapis.com https://accounts.google.com/gsi/client; object-src 'self'",
	"chrome_url_overrides" : {
		"newtab": "index.html"
	},
	"oauth2": {
		"client_id": "475642402722-m6pkaq5ks6otsnk4396o74j7t2o70vfg.apps.googleusercontent.com",
		"scopes": [
			"profile email",
			"https://www.googleapis.com/auth/contacts"
		]
	},
	"background": {
        "scripts": [
            "js/background.js"
        ]
    },
	"permissions":[
		"identity",
		"identity.email"
	]
}
